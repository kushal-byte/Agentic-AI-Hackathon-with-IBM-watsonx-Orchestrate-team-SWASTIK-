# Environment Variables Template
# Copy this to .env and fill in your API keys

# OpenRouter API Key (Primary - for Grok 4.1 Fast)
VITE_OPENROUTER_API_KEY=sk-or-v1-your-api-key-here

# IBM Watson Orchestrate API Keys (Secondary)
VITE_WATSON_API_KEY=your-watson-api-key-here
VITE_WATSON_PROJECT_ID=your-watson-project-id-here
VITE_WATSON_URL=your-watson-url-here

# Optional: Debug mode (set to true for development)
VITE_DEBUG_MODE=false